@page "/checkers/{userId}/{date}"
@layout MainLayoutEmpty

<HeadContent>
    <meta name="robots" content="noindex, nofollow" />
</HeadContent>

<CheckerDetailHistoryComponent userId="@userId" date="@date" @rendermode="new InteractiveWebAssemblyRenderMode(prerender: true)" />

@code {
    [Parameter]
    public string userId { get; set; } = string.Empty;
    [Parameter]
    public string date { get; set; } = string.Empty;
}
